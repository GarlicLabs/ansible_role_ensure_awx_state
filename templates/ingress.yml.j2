apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: awx-{{ awx_name }}-ingress
  namespace: {{ awx_namespace }} 
spec:
  ingressClassName: nginx
  rules:
  - host: {{ awx_hostname }}
    http:
      paths:
      - path: /
        pathType: Prefix
        backend:
          service:
            name: {{ awx_name }}-service
            port:
              number: 80